{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-console": false,
        "trailing-comma": false,
        "callable-types": false,
        "new-parens": false,
        "ordered-imports": false,
        "indent": false,
        "object-literal-sort-keys": false,
        "interface-name": false
    },
    "rulesDirectory": []
}